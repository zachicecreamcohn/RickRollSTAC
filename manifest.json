{
    "name": "NewSTAC",
    "description": "A more beautiful version of WebSTAC",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./images/rickroll_icon.png",
        "32": "./images/rickroll_icon.png",
        "48": "./images/rickroll_icon.png",
        "128": "./images/rickroll_icon.png"
    },
    "background": {
        "scripts": [
            "./js/background.js"
        ]
    },
    "browser_action": {
        "default_popup": "./popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "*://acadinfo.wustl.edu/*"
    ],
    "web_accessible_resources": [
        "audio/rickroll.mp3"
    ]



}